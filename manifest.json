{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "19.png",
      "default_popup": "popup.html",
      "default_title": "ESI Stylish"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "apply.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "js": [ "install.js" ],
      "matches": [ "http://userstyles.org/*", "https://userstyles.org/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_description__",
   "developer": {
      "name": "danillatech"
   },
   "homepage_url": "http://dreamject.ru/dreamjects/esi/support/",
   "icons": {
      "128": "128.png",
      "16": "16.png",
      "48": "48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ESI Stylish",
   "options_page": "manage.html",
   "permissions": [ "tabs", "http://userstyles.org/", "https://userstyles.org/" ],
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "1.1.3"
}
